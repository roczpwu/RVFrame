<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN" "http://getahead.org/dwr/dwr30.dtd">

<dwr>

  <allow>
    <create creator="new" javascript="AjaxInterface">
      <param name="class" value="com.roc.ajax.AjaxInterface" />
    </create>
    <convert match="com.roc.core.BLMessage" converter="bean" javascript="BLMessage"/>
    <convert match="java.lang.Exception" converter="exception"/>
    <convert match="java.lang.StackTraceElement" converter="bean"/>
    <convert match="java.lang.Object" converter="bean"/>
    <convert match="freemarker.template.TemplateException" converter="exception"/>
  </allow>

</dwr>
